<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	
	<bean id="cusDb" class="org.roof.excel.config.XslDb">
		<property name="columns">
			<list>
				<bean class="org.roof.excel.config.Column">
					<property name="xslcol" value="A" />
					<property name="dbcol" value="date_day" />
					<property name="title" value="" />
				</bean>
				<bean class="org.roof.excel.config.Column">
					<property name="xslcol" value="B" />
					<property name="dbcol" value="code" />
					<property name="title" value="" />
				</bean>
				<bean class="org.roof.excel.config.Column">
					<property name="xslcol" value="C" />
					<property name="dbcol" value="pro_name" />
					<property name="title" value="" />
				</bean>
				<bean class="org.roof.excel.config.Column">
					<property name="xslcol" value="D" />
					<property name="dbcol" value="pro_norms" />
					<property name="title" value="" />
				</bean>
				<bean class="org.roof.excel.config.Column">
					<property name="xslcol" value="E" />
					<property name="dbcol" value="num" />
					<property name="title" value="" />
				</bean>
				<bean class="org.roof.excel.config.Column">
					<property name="xslcol" value="F" />
					<property name="dbcol" value="price" />
					<property name="title" value="" />
				</bean>
				<bean class="org.roof.excel.config.Column">
					<property name="xslcol" value="G" />
					<property name="dbcol" value="amount" />
					<property name="title" value="" />
				</bean>
			</list>
		</property>
	</bean>

</beans>